{
  "name": "client",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier -w pages utils components styles context --single-quote"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "ethers": "^5.5.4",
    "framer-motion": "^6.5.1",
    "next": "^12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-marquee": "^1.3.2",
    "react-icons": "^4.4.0",
    "swiper": "^8.3.1",
    "web3": "^1.6.1",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "prettier": "^2.5.1"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@helpers/*": [
        "helpers/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
